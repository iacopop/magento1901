<?xml version="1.0" encoding="UTF-8"?>
<config>
    <api>
        <resources>
            <iris_resource translate="title" module="iris">
	            <title>Data Exchange WS</title>
	            <model>iris/api_resource</model>
	            <methods>
	                <hello translate="title" module="iris">
	                    <title>Hello</title>
	                    <method>hello</method>
                            <acl>iris/resource/hello</acl>
	                </hello>                      
	                <exporttest translate="title" module="iris">
