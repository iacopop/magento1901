                            <duplicateproduct translate="title" module="iris">
                                <title>Duplicate Product</title>
                            </duplicateproduct>                                                
                            <importbrand translate="title" module="iris">
                                <title>Import Brand</title>
                            </importbrand>                        
                            <importstockexpiry translate="title" module="iris">
                                <title>Import Stock and Expiration</title>
